			result = mprint(s_INFO_NEW_CYCLE_247)
			result = mprint(s_INFO_NEW_CYCLE_247_2)
			nbr_info_err_1 = 1 
			]
		old_mi1 = mi1_prmcode
		]
	# OPERATIONS TRANSFORMEES
	if xform_op_id$ <> op_id$ & workofs$ <> old_workofs & workofs_dec > 0, [	
		workofs_ = workofs$
		if Type_decalages = 1 | debut = 2, 	[	# 1er en 247
