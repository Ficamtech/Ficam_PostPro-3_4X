    if nextop$ <> 11, [      # = positionné    Si pas en 5 axes      
		x$ = xr$
        y$ = yr$
        z$ = zr$
        xabs = x$
        yabs = y$
        zabs = z$
        if rotaxis$ = 0, [  # 4 axes positionné
            if diviseur <> 0 & rotation_axe <> -9999, [
                p_calcul_rot_posit
                p_rot_tlchg0
                ]
            ]
        if rotaxis$ <> 0, 	# si on passe de positionné à SUBSTITUTION
			[
