		p_calcul_AB_pos_Subs
        @depth$
        @refht$
		prof_abs = (rotdia$/2) - abs(depth$)
        prof_inc = depth$ - ((rotdia$/2) - refht$) 
        garde_abs = refht$ + (rotdia$/2)
